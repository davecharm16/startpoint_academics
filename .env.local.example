# Supabase Configuration
# Get these values from: npx supabase status (after running npx supabase start)
NEXT_PUBLIC_SUPABASE_URL=http://localhost:54321
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_DEFAULT_KEY=<your-publishable-key>
SUPABASE_SERVICE_ROLE_KEY=<your-service-role-key>

# Application URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Email Service (Resend)
RESEND_API_KEY=<your-resend-api-key>
EMAIL_FROM=Startpoint Academics <noreply@startpointacademics.com>

# Admin Configuration
ADMIN_EMAIL=admin@startpointacademics.com
ADMIN_NAME=Admin

# Cron Job Security (generate a random string)
CRON_SECRET=<your-random-cron-secret>
